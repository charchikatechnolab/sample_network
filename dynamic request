function insertquery(pagetitle){
  let url_str = "";
  var pkg = JSON.stringify({
    title:pagetitle,
    region:Intl.DateTimeFormat().resolvedOptions().timeZone
  });
  let form = new FormData();
  form.append('json',pkg);
  if(window.location.origin == 'https://quickunitconverter.com'){
    fetch('https://quickunitconverter.com/view/analysis.php',{
      method:'POST',
      body:form
    })
    .then(response => response.text())
    .then(data => console.log(data));
  }
}

<?php
if (isset($_POST)) {
   include $_SERVER['DOCUMENT_ROOT'] . "/common/CommonFunction.php";
   include CONTROLLERS_PATH . "/IndexUnit.php";
   $obj_index = new IndexUnit("analysis");
   $json = json_decode($_POST['json'],true);
   $title =  $json['title'];
   $region = $json['region'];
   $obj_index->analysis($title,$region);
}
?>
